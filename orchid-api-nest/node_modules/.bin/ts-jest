#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/awad/Main.OS.Files/Dev/WebDev/Projects/FullStack/FullStack_TS/Orchid-React-Nestjs/orchid-api-nest/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_ef776c0ffe6fd122603e7e241192168f/node_modules/ts-jest/node_modules:/home/awad/Main.OS.Files/Dev/WebDev/Projects/FullStack/FullStack_TS/Orchid-React-Nestjs/orchid-api-nest/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_ef776c0ffe6fd122603e7e241192168f/node_modules:/home/awad/Main.OS.Files/Dev/WebDev/Projects/FullStack/FullStack_TS/Orchid-React-Nestjs/orchid-api-nest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/awad/Main.OS.Files/Dev/WebDev/Projects/FullStack/FullStack_TS/Orchid-React-Nestjs/orchid-api-nest/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_ef776c0ffe6fd122603e7e241192168f/node_modules/ts-jest/node_modules:/home/awad/Main.OS.Files/Dev/WebDev/Projects/FullStack/FullStack_TS/Orchid-React-Nestjs/orchid-api-nest/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_ef776c0ffe6fd122603e7e241192168f/node_modules:/home/awad/Main.OS.Files/Dev/WebDev/Projects/FullStack/FullStack_TS/Orchid-React-Nestjs/orchid-api-nest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_ef776c0ffe6fd122603e7e241192168f/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_ef776c0ffe6fd122603e7e241192168f/node_modules/ts-jest/cli.js" "$@"
fi
